{
  "name": "rx-react-container",
  "version": "0.7.0",
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "description": "Provides HoC component, and utilities to connect RxJS logic to React Component.",
  "keywords": [
    "react",
    "rxjs",
    "container",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zxbodya/rx-react-container.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "main": "dist/index.js",
  "module": "dist/rx-react-container.esm.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint --project .",
    "cover": "jest --coverage",
    "test": "jest",
    "build": "rimraf dist && rollup -c",
    "watch": "rollup -c -w",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.10",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.7.21",
    "codecov.io": "^0.1.6",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "prop-types": "^15.6.2",
    "raf": "^3.4.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "16.7.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.1.2",
    "rollup-plugin-typescript2": "^0.19.2",
    "rxjs": "^6.3.3",
    "temp-dir": "^1.0.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.1"
  },
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "rxjs": "^6.2.0",
    "tslib": "^1.9.3"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "preset": "ts-jest",
    "setupFiles": [
      "raf/polyfill",
      "./setupJest"
    ],
    "roots": [
      "src"
    ]
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.0"
  }
}
